<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.checkbox {
			color: #646464;
		}
		
		.main-panel {
			padding-left: 10px;
		}
	</ui:style>
	<g:HorizontalPanel styleName='{style.main-panel}'>
		<g:CheckBox styleName='{style.checkbox}' ui:field="checkbox" />
	</g:HorizontalPanel>

</ui:UiBinder>