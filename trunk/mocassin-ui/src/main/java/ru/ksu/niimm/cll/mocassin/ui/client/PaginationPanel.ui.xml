<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.pagination-panel {
			text-align: center;
			width: 100%;
		}
		
		.pagination-table {
			text-align: center;
		}
	</ui:style>
	<g:HorizontalPanel styleName='{style.pagination-panel}'>
		<g:FlexTable ui:field="table" styleName='{style.pagination-table}' />
	</g:HorizontalPanel>
</ui:UiBinder> 