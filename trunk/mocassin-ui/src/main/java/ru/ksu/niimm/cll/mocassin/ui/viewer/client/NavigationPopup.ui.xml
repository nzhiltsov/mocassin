<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.goLink {
			font-size: large;
			color: silver;
			cursor: pointer;
			text-decoration: underline;
		}
	</ui:style>
	<g:HorizontalPanel>
		<g:PopupPanel ui:field="popup">
			<g:Anchor ui:field="goLink" styleName='{style.goLink}' />
		</g:PopupPanel>
	</g:HorizontalPanel>
</ui:UiBinder>


