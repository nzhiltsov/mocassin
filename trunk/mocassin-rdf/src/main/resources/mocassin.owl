            </owl:Class>
        </rdfs:range>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://cll.niimm.ksu.ru/ontologies/mocassin#DocumentSegment"/>
