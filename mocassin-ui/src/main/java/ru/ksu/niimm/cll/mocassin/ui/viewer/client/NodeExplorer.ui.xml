<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
              xmlns:g='urn:import:com.google.gwt.user.client.ui' 
              xmlns:v='urn:import:ru.ksu.niimm.cll.mocassin.ui.viewer.client'>
    <v:NodeExplorer text="NodeExplorer" animationEnabled="true" glassEnabled="true" visible="false">
        <g:VerticalPanel horizontalAlignment="ALIGN_RIGHT">
            <g:HTML>
                <b>Sending name to the server:</b>
            </g:HTML>
            <g:Label ui:field="labelTextToServer" />
            <g:HTML>
                <br/>
                <b>Server replies:</b>
            </g:HTML>
            <g:Button ui:field="closeButton" text="Close"/>
        </g:VerticalPanel>
    </v:NodeExplorer>
</ui:UiBinder>