  <rdf:Description rdf:about="http://omdoc.org/ontology#SequentPart">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#Statement"/>
    <dc:description>A part (either assumption or conclusion) of a sequent structure that a formal property may have</dc:description>
    <rdfs:label xml:lang="de">Sequenzabschnitt</rdfs:label>
    <rdfs:label xml:lang="en">Sequent Part</rdfs:label>
    <rdfs:label xml:lang="ru">Следующая часть</rdfs:label>
    <owl:disjointWith rdf:resource="http://omdoc.org/ontology#StatementInTheory"/>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#Hypothesis">
    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#GeneralAxiom"/>
    <dc:description>Local assumptions that allow hypothetical reasoning, as defined in the OMDoc 1.2 manual, chapter 17 (Representing Proofs).</dc:description>
    <owl:disjointWith rdf:resource="http://omdoc.org/ontology#DerivationStep"/>
    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#FormalKnowledgeItem"/>
    <rdfs:label xml:lang="en">Hypothesis</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#ProofLocalStatement"/>
    <rdfs:label xml:lang="de">Hypothese</rdfs:label>
    <rdfs:label xml:lang="ru">Гипотеза</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A9">
    <rdf:first rdf:resource="http://omdoc.org/ontology#usesSymbol"/>
    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A2">
