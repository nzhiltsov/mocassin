    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#ConstitutiveStatement"/>
    <dc:description>An import from one theory into another one, modeled as a first-class object</dc:description>
    <rdfs:label xml:lang="de">Import</rdfs:label>
    <rdfs:label xml:lang="en">Import</rdfs:label>
    <owl:disjointWith rdf:resource="http://omdoc.org/ontology#Symbol"/>
    <owl:disjointWith rdf:resource="http://omdoc.org/ontology#Axiom"/>
    <owl:disjointWith rdf:resource="http://omdoc.org/ontology#Definition"/>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#occursIn">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <owl:inverseOf rdf:resource="http://omdoc.org/ontology#usesSymbol"/>
    <rdfs:label xml:lang="de">kommt vor in</rdfs:label>
    <rdfs:label xml:lang="en">occurs in</rdfs:label>
    <rdfs:label xml:lang="ru">встречается в</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#hasAssertedType">
