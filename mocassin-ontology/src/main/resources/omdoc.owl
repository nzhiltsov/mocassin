  <rdf:Description rdf:nodeID="A14">
    <rdf:first rdf:resource="http://omdoc.org/ontology#hasType"/>
    <rdf:rest rdf:nodeID="A9"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#TextFragment">
