    <rdfs:subPropertyOf rdf:resource="http://omdoc.org/ontology#wellFormedNessDependsOn"/>
    <rdfs:domain rdf:resource="http://omdoc.org/ontology#Symbol"/>
    <rdfs:range rdf:resource="http://omdoc.org/ontology#Symbol"/>
    <owl:propertyChainAxiom rdf:nodeID="A13"/>
    <rdfs:label xml:lang="de">enthält in der Definition</rdfs:label>
    <rdfs:label xml:lang="en">has occurrence of in definition</rdfs:label>
    <dc:description>Another symbol occurs in the definition of this symbol (MMT/OMDoc1.6)</dc:description>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#hasDeclaredType">
