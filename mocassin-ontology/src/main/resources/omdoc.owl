    <rdfs:label xml:lang="de">begründet durch Unterbeweis</rdfs:label>
    <rdfs:label xml:lang="en">justified by subproof</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
