    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#SequentPart"/>
    <dc:description>Represents any assumption that may occur in a formal property.</dc:description>
    <rdfs:label xml:lang="de">Vermutung</rdfs:label>
    <rdfs:label xml:lang="en">Assumption</rdfs:label>
    <rdfs:label xml:lang="ru">Предположение</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#AnyLevelStatement">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#Statement"/>
    <dc:description>Generic superclass of statements that can not only occur as children of theories but also elsewhere.</dc:description>
    <rdfs:label xml:lang="de">Aussage</rdfs:label>
    <rdfs:label xml:lang="en">Statement</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#Type">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#FormalKnowledgeItem"/>
    <dc:description>The type of a symbol (declared or asserted), as defined in the OMDoc1.2 specification, chapter 15.2.3 (Type Declarations) and 15.3.2 (Type Assertions).
In OMDoc/XML, this is represented by the omdoc:type element.</dc:description>
    <owl:disjointWith rdf:resource="http://omdoc.org/ontology#Theory"/>
    <rdfs:label xml:lang="de">Typ</rdfs:label>
    <rdfs:label xml:lang="en">Type</rdfs:label>
    <rdfs:label xml:lang="ru">Тип</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
