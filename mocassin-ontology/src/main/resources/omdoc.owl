  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#logic">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="http://omdoc.org/ontology#Property"/>
    <dc:description>A Formal Mathematical Property is formally represented in some logic. This property helps us create a relation between the property and the logic. This is as defined in the OMDoc 1.2 specification, chapter 1 (Mathematical Text (Modules MTXT and RT)</dc:description>
    <rdfs:label xml:lang="de">Logik</rdfs:label>
    <rdfs:label xml:lang="en">Logic</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A5">
    <rdf:first rdf:resource="http://omdoc.org/ontology#hasImport"/>
    <rdf:rest rdf:nodeID="A6"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#hasDirectPart">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <dc:description>We model the direct relation between a whole and a part separately, as that allows us to state that it is inverse functional.</dc:description>
    <rdfs:label xml:lang="de">hat direkten Teil</rdfs:label>
    <rdfs:label xml:lang="en">has direct part</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#stepLocallyJustifiedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://omdoc.org/ontology#stepJustifiedBy"/>
    <rdfs:range rdf:resource="http://omdoc.org/ontology#Statement"/>
    <dc:description>Points from a derivation step in a proof to (an)other local proof step(s) or to a nested proof (i.e. to substeps) giving its justification</dc:description>
    <rdfs:label xml:lang="de">begründet durch</rdfs:label>
    <rdfs:label xml:lang="en">justified by</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#hasProperty">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://omdoc.org/ontology#hasDirectPart"/>
    <rdfs:domain rdf:resource="http://omdoc.org/ontology#Statement"/>
    <rdfs:range rdf:resource="http://omdoc.org/ontology#Property"/>
    <dc:description>Represents relation between nodes and its CMP/FMP children nodes</dc:description>
    <rdfs:label xml:lang="de">hat Eigenschaft</rdfs:label>
    <rdfs:label xml:lang="en">has Property</rdfs:label>
    <rdfs:label xml:lang="ru">обладает свойством</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://omdoc.org/ontology#MathKnowledgeItem">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dc:description>Any mathematical knowledge item that can be expressed in OMDoc (i.e. a statement or theory, or probably something else).  This base class neither has a counterpart in the OMDoc1.2 specification nor in its XML schema.</dc:description>
    <rdfs:label xml:lang="de">Mathematische Wissenseinheit</rdfs:label>
    <rdfs:label xml:lang="en">Mathematical Knowledge Item</rdfs:label>
    <rdfs:label xml:lang="ru">Объект математического знания</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="http://omdoc.org/ontology#"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="A7">
