    <rdfs:subPropertyOf rdf:resource="http://omdoc.org/ontology#hasType"/>
    <rdfs:range rdf:resource="http://omdoc.org/ontology#DeclaredType"/>
    <dc:description>The relation between a symbol and its declared type, as defined in the OMDoc1.2 specification, chapter 15.2.3 (Type Declarations).</dc:description>
    <rdfs:label xml:lang="de">hat deklarierten Typ</rdfs:label>
    <rdfs:label xml:lang="en">has declared type</rdfs:label>
