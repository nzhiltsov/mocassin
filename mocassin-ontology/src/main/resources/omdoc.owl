    <rdfs:subClassOf rdf:resource="http://omdoc.org/ontology#Reference"/>
    <owl:disjointWith rdf:resource="http://omdoc.org/ontology#Citation"/>
    <dc:description>Some references hace the include type attribute, meaning they are text fragments to be replaced in the text.</dc:description>
